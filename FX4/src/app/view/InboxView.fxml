<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.AdminInboxController">
   <top>
      <AnchorPane prefHeight="600.0" prefWidth="500.0" style="-fx-border-color: #99bbff;" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="-17.0" layoutY="49.0" prefHeight="90.0" prefWidth="365.0" style="-fx-background-color: #f2ffe6;" />
            <HBox layoutX="13.0" layoutY="330.0" prefHeight="50.0" prefWidth="469.0" />
            <Label layoutX="25.0" layoutY="44.0" prefHeight="32.0" prefWidth="400.0" text="ADMIN PANEL" textFill="#5e5858fc">
               <font>
                  <Font name="Arial Black" size="34.0" />
               </font>
            </Label>
            <ImageView fx:id="iv_back" fitHeight="37.0" fitWidth="42.0" layoutX="449.0" layoutY="556.0" onMouseClicked="#action_back" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../Downloads/icons8-Back%20To-50.png" />
               </image>
            </ImageView>
            <TableView fx:id="TableInbox" layoutX="11.0" layoutY="170.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="483.0">
              <columns>
                <TableColumn fx:id="tb_from" prefWidth="67.0" text="from" />
                <TableColumn fx:id="tb_to" minWidth="6.0" prefWidth="70.0" text="to" />
                  <TableColumn fx:id="tb_subject" prefWidth="112.0" text="subject" />
                  <TableColumn fx:id="tb_content" prefWidth="233.0" text="content" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               
         
            </TableView>
            <Button fx:id="get_messages" layoutX="58.0" layoutY="390.0" mnemonicParsing="false" onMouseClicked="#action_getMessages" prefHeight="58.0" prefWidth="117.0" text="Get messages" textAlignment="CENTER" wrapText="true" />
            <Button layoutX="364.0" layoutY="10.0" lineSpacing="5.0" mnemonicParsing="false" prefHeight="102.0" prefWidth="127.0" style="-fx-background-color: #e6e6e6; -fx-border-color: #c2c2d6;" text="Inbox" textAlignment="CENTER" textFill="#615d5d" wrapText="true">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
               <effect>
                  <DropShadow />
               </effect>
            </Button>
            <Button fx:id="get_delete" layoutX="280.0" layoutY="390.0" mnemonicParsing="false" onMouseClicked="#action_delete" prefHeight="58.0" prefWidth="117.0" text="Delete message" textAlignment="CENTER" wrapText="true" />
            <Label layoutX="52.0" layoutY="78.0" prefHeight="69.0" prefWidth="208.0" text="M E N U" textFill="#c93030fc">
               <font>
                  <Font name="Arial Black" size="46.0" />
               </font>
            </Label>
           
         </children>
      </AnchorPane>
   </top>
</BorderPane>
